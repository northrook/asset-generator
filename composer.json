{
  "name": "northrook/assets",
  "description": "An asset generator written in PHP. Creates optimised, public versions of assets from various sources.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Northrook\\Assets\\": "src"
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "northrook/core": "dev-main",
    "northrook/minify": "dev-main",
    "northrook/html-element": "dev-main",
    "northrook/filesystem": "dev-main",
    "northrook/settings": "dev-main",
    "northrook/clerk": "dev-main"
  },
  "require-dev": {
    "northrook/dev-env": "dev-main",
    "symfony/cache": "^7.1",
    "phpstan/phpstan": "^1.12"
  },
  "config": {
  }
}
