{
  "name": "northrook/assets",
  "description": "An asset generator written in PHP. Creates optimised, public versions of assets from various sources.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Core\\Assets\\": "src"
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "northrook/pathfinder": "dev-main",
    "northrook/minify": "dev-main",
    "northrook/view": "dev-main"
  },
  "require-dev": {
    "symfony/framework-bundle": "@stable",
    "latte/latte": "@stable",
    "northrook/dev-env": "dev-main",
    "phpstan/phpstan": "@stable",
    "northrook/settings": "dev-main"
  }
}
